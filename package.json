{
  "name": "pharma-chain-application",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "start:prod": "npm run build:prod && npm run server",
    "heroku-postbuild": "npm run build:prod",
    "start": "node ./projects/server/server.js",
    "lint": "ng lint",
    "test": "ng lint && ng test --configuration=test",
    "watch": "ng test --configuration=test --browsers ChromeHeadless --watch",
    "e2e": "ng e2e",
    "ci": "npm run format:test && ng lint && ng test --configuration=test --browsers ChromeTravisCi --code-coverage && npm run e2e && npm run build:prod",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "release": "standard-version && git push --follow-tags origin master",
    "analyze": "npm run build:prod -- --stats-json && webpack-bundle-analyzer ./dist/pharma-chain-application/stats-es2015.json",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate && node .all-contributors-html.js",
    "contributors:check": "all-contributors check"
  },
  "private": true,
  "dependencies": {
    "@angular-extensions/elements": "^8.10.0",
    "@angular-extensions/model": "^8.0.0",
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.0.0",
    "@angular/http": "^8.0.0-beta.10",
    "@angular/material": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@auth0/auth0-spa-js": "^1.5.0",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@ngrx/effects": "^8.5.0",
    "@ngrx/entity": "^8.5.0",
    "@ngrx/router-store": "^8.5.0",
    "@ngrx/store": "^8.5.0",
    "@ngrx/store-devtools": "^8.5.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.4.1",
    "browser-detect": "^0.2.28",
    "compression": "^1.7.4",
    "express": "^4.16.4",
    "hammerjs": "^2.0.8",
    "ng2-pdf-viewer": "^5.3.4",
    "rxjs": "^6.5.4",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@commitlint/cli": "^8.3.3",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jasmine": "~2.8.9",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^10.17.5",
    "@types/uuid": "^3.4.6",
    "all-contributors-cli": "^5.4.1",
    "codelyzer": "^5.2.0",
    "husky": "^1.1.3",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "karma-spec-reporter": "^0.0.32",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "protractor": "~5.4.1",
    "rimraf": "^2.6.2",
    "standard-version": "^6.0.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.15.0",
    "typescript": "~3.4.0",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
